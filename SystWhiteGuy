  local whitelistecheck = loadstring(game:HttpGet("https://raw.githubusercontent.com/vanya6583/LAVA-LUCKY-BLOCKS-Battlegrounds/main/PlayerseSyst", true))()

if whitelistecheck[game:service('Players').LocalPlayer.UserId] then
  game:service('Players').LocalPlayer:Kick('You are locked in the script or the script is being updated')
else
print("{{Script by BUNBASTER12BLASTER}}")
wait(0.2)
loadstring(game:HttpGet("https://raw.githubusercontent.com/vanya6583/LAVA-LUCKY-BLOCKS-Battlegrounds/main/189583103", true))()
end
